            {
              "name": "Over the Top",
              "description": "Walk laterally over the bench.",
              "cue": "Over the top / Dall'altra parte"
            },
            {
              "name": "Mambo",
              "description": "Rock forward on the floor/bench, recover, step back.",
              "cue": "Mambo!"
